{
	"schemaVersion": 1,
	"id": "quic_protocol_support",
	"version": "${version}",
	"name": "QuicProtocolSupport",
	"description": "Adds support for Minecraft over QUIC",
	"authors": [
		"tesinormed",
		"ramidzkh"
	],
	"contact": {
		"homepage": "https://github.com/tesinormed/quic_protocol_support",
		"issues": "https://github.com/tesinormed/quic_protocol_support/issues",
		"sources": "https://github.com/tesinormed/quic_protocol_support"
	},
	"license": "LGPL-3.0-or-later",
	"icon": "assets/icon.png",
	"environment": "*",
	"mixins": [
		{
			"environment": "*",
			"config": "quic_protocol_support.mixins.json"
		},
		{
			"environment": "client",
			"config": "quic_protocol_support.client.mixins.json"
		}
	],
	"depends": {
		"fabricloader": ">=${loader_version}",
		"fabric-language-kotlin": ">=${kotlin_loader_version}",
		"minecraft": "${minecraft_version}"
	}
}
